{
  "name": "@dnb-org-libraries/heroicons",
  "description": "Heroicons for Hugo",
  "version": "0.0.0",
  "author": "Patrick Kollitsch <patrick@davids-neighbour.com>",
  "bugs": {
    "url": "https://github.com/dnb-org/libraries/issues"
  },
  "devDependencies": {
    "@dnb-org/standard-version-config": "3.1.0",
    "standard-version": "9.3.0"
  },
  "homepage": "https://github.com/dnb-org/libraries/tree/master/heroicons",
  "license": "MIT",
  "private": true,
  "scripts": {
    "release": "standard-version --release-as patch -a -t \"heroicons/v\" --releaseCommitMessageFormat \"chore(release): bootstrap-icons/v{{currentTag}}\" && ./bin/release-hook-postrelease.sh",
    "release:major": "standard-version --release-as major -a -t \"heroicons/v\" --releaseCommitMessageFormat \"chore(release): bootstrap-icons/v{{currentTag}}\" && ./bin/release-hook-postrelease.sh",
    "release:next": "standard-version --release-as minor -a -t \"heroicons/v\" --releaseCommitMessageFormat \"chore(release): bootstrap-icons/v{{currentTag}}\" && ./bin/release-hook-postrelease.sh"
  }
}
