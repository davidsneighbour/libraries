{
  "name": "@davidsneighbour/bootstrap5",
  "description": "Bootstrap 5 for Hugo",
  "version": "2.2.5",
  "author": "Patrick Kollitsch <patrick@davids-neighbour.com>",
  "bugs": {
    "url": "https://github.com/davidsneighbour/libraries/issues"
  },
  "commitlint": {
    "extends": [
      "@davidsneighbour/commitlint-config"
    ]
  },
  "devDependencies": {
    "@davidsneighbour/browserslist-config": "4.0.0",
    "@davidsneighbour/commitlint-config": "4.0.0",
    "@davidsneighbour/eslint-config": "4.0.0",
    "@davidsneighbour/remark-config": "4.0.0",
    "@davidsneighbour/standard-version-config": "4.0.0",
    "@davidsneighbour/stylelint-config": "4.0.0",
    "@davidsneighbour/textlint-config": "4.0.9"
  },
  "homepage": "https://github.com/davidsneighbour/libraries/tree/master/bootstrap5",
  "license": "MIT",
  "private": true,
  "scripts": {
    "release": "standard-version --release-as patch -a -t \"bootstrap5/v\" --releaseCommitMessageFormat \"chore(release): bootstrap5/v{{currentTag}}\" && ./bin/release-hook-postrelease.sh",
    "release:major": "standard-version --release-as major -a -t \"bootstrap5/v\" --releaseCommitMessageFormat \"chore(release): bootstrap5/v{{currentTag}}\" && ./bin/release-hook-postrelease.sh",
    "release:next": "standard-version --release-as minor -a -t \"bootstrap5/v\" --releaseCommitMessageFormat \"chore(release): bootstrap5/v{{currentTag}}\" && ./bin/release-hook-postrelease.sh"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.5"
  }
}
