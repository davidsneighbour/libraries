{
  "name": "@dnb-hugo-libraries/bootstrap-icons",
  "description": "Bootstrap Icons for Hugo",
  "version": "1.3.3",
  "author": "Patrick Kollitsch <patrick@davids-neighbour.com>",
  "bugs": {
    "url": "https://github.com/dnb-hugo/libraries/issues"
  },
  "devDependencies": {
    "@dnb-hugo/standard-version-config": "github:dnb-hugo/standard-version-config",
    "standard-version": "9.1.1"
  },
  "homepage": "https://github.com/dnb-hugo/libraries/tree/master/bootstrap-icons",
  "license": "MIT",
  "private": true,
  "scripts": {
    "release": "standard-version --release-as patch -a -t \"bootstrap-icons/v\" --releaseCommitMessageFormat \"chore(release): bootstrap-icons/v{{currentTag}}\" && ./bin/release-hook-postrelease.sh",
    "release-major": "standard-version --release-as major -a -t \"bootstrap-icons/v\" --releaseCommitMessageFormat \"chore(release): bootstrap-icons/v{{currentTag}}\" && ./bin/release-hook-postrelease.sh",
    "release-next": "standard-version --release-as minor -a -t \"bootstrap-icons/v\" --releaseCommitMessageFormat \"chore(release): bootstrap-icons/v{{currentTag}}\" && ./bin/release-hook-postrelease.sh"
  }
}
